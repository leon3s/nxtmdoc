{
  "name": "nxtmdoc",
  "bin": {
    "nxtmdoc": "./bin/nxtmdoc.js"
  },
  "scripts": {
    "dev": "node ./server.js",
    "lint": "next lint",
    "build": "next build",
    "pkg": "pkg -t node16-linux-x64 --compress GZip .",
    "hygen": "hygen",
    "test": "exit 0",
    "start": "node ./server.js"
  },
  "pkg": {
    "assets": [
      ".next/**/*"
    ],
    "scripts": [
      ".next/dist/**/*.js"
    ],
    "targets": [
      "node16-linux-x64"
    ],
    "outputPath": "dist"
  },
  "dependencies": {
    "@mui/base": "^5.0.0-alpha.98",
    "@mui/lab": "^5.0.0-alpha.100",
    "@mui/material": "^5.10.6",
    "@swc/core": "^1.3.2",
    "@swc/register": "^0.1.10",
    "commander": "^9.4.0",
    "execa": "^6.1.0",
    "express": "^4.18.1",
    "react-markdown": "^8.0.3",
    "react-syntax-highlighter": "^15.5.0",
    "rehype-raw": "^6.1.1",
    "remark-gfm": "^3.0.1",
    "yaml": "^2.1.1"
  },
  "devDependencies": {
    "@emotion/react": "^11.10.4",
    "@emotion/styled": "^11.10.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "next": "^12.3.1",
    "@types/node": "^18.7.18",
    "@types/react": "^18.0.20",
    "@types/react-dom": "^18.0.6",
    "@types/react-syntax-highlighter": "^15.5.5",
    "eslint": "^8.23.1",
    "eslint-config-next": "^12.3.1",
    "hygen": "^6.2.11",
    "typescript": "^4.8.3"
  }
}
